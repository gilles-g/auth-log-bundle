{
  "name": "spiriit-auth-log-configurator",
  "version": "1.0.0",
  "description": "Interactive CLI to configure SpiriitAuthLogBundle YAML configuration",
  "type": "module",
  "bin": {
    "spiriit-auth-log-config": "./dist/index.js"
  },
  "scripts": {
    "start": "npm run build && node dist/index.js",
    "dev": "npm run build && node dist/index.js",
    "build": "esbuild index.js --bundle --platform=node --format=esm --outfile=dist/index.js --external:react --external:ink --external:ink-text-input --external:ink-select-input --external:yaml --loader:.js=jsx --banner:js='#!/usr/bin/env node'",
    "prebuild": "node -e \"require('fs').mkdirSync('dist', { recursive: true })\""
  },
  "keywords": [
    "spiriit",
    "auth-log",
    "symfony",
    "configuration",
    "cli"
  ],
  "author": "Spiriit",
  "license": "MIT",
  "dependencies": {
    "ink": "^5.0.1",
    "ink-text-input": "^6.0.0",
    "ink-select-input": "^6.0.0",
    "react": "^18.3.1",
    "yaml": "^2.6.1"
  },
  "devDependencies": {
    "esbuild": "^0.24.2"
  }
}
